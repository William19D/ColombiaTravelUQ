<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.alquiler.controller.RegistroVehiculoController">
    <children>
        <Label layoutY="57.0" prefHeight="18.0" text="Placa:" />
        <TextField fx:id="txtPlaca" layoutX="108.0" layoutY="53.0" />

        <Label layoutY="100.0" prefHeight="18.0" text="Referencia:" />
        <TextField fx:id="txtReferencia" layoutX="108.0" layoutY="96.0" />

        <Label layoutY="144.0" prefHeight="18.0" text="Marca:" />
        <ComboBox fx:id="opcionesMarca" layoutX="108.0" layoutY="140.0" prefWidth="162.0" />

        <Label layoutY="188.0" prefHeight="18.0" text="Modelo:" />
        <TextField fx:id="txtModelo" layoutX="108.0" layoutY="184.0" />

        <Label layoutY="231.0" prefHeight="18.0" text="Url foto:" />
